# source 'https://github.com/CocoaPods/Specs.git'

# Uncomment the next line to define a global platform for your project
platform :ios, '8.0'

target 'SkyKit' do
  
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
   use_frameworks!
   
  # Pods for SkyKit
  pod 'Alamofire'
  pod 'SwiftyJSON'
  pod 'SwiftyBeaver’
  pod 'Reachability’
  pod 'MJRefresh'
  pod 'SDWebImage', '~>3.8'
  pod 'IQKeyboardManagerSwift'  # 键盘工具
  pod 'BRYXBanner'              # topbar 提示，轻量级下拉横幅展示 http://www.github.com/bryx-inc/BRYXBanner
  pod 'SnapKit'                 # 自动布局库
  pod 'MBProgressHUD'
  pod 'CoreStore'               # 数据存储
  
  #  pod 'MLeaksFinder'
  #  pod 'CryptoSwift', '~>0.7.1' #加密库
end

inhibit_all_warnings!
